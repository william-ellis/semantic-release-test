{
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "semantic-release": "19.0.5"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/exec",
        {
          "pulishCmd": "./Publish.ps1 ./output/Test.psd1 ${nextRelease.version} -Verbose",
          "shell": "pwsh"
        }
      ]
    ]
  }
}
